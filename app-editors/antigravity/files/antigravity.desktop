[Desktop Entry]
Name=Antigravity
Comment=AI-powered code editor based on Visual Studio Code
GenericName=Text Editor
Exec=antigravity --no-sandbox --unity-launch %F
Icon=antigravity
Type=Application
StartupNotify=true
StartupWMClass=Antigravity
Categories=Utility;Development;IDE;TextEditor;
MimeType=text/plain;text/x-chdr;text/x-csrc;text/x-c++hdr;text/x-c++src;text/x-java;text/x-dsrc;text/x-pascal;text/x-perl;text/x-python;application/x-php;application/x-httpd-php3;application/x-httpd-php4;application/x-httpd-php5;application/xml;text/html;text/css;text/x-sql;text/x-diff;text/javascript;application/json;text/x-markdown;text/x-go;text/x-rust;text/x-ruby;text/x-sh;text/x-tex;text/x-yaml;
Actions=new-empty-window;
Keywords=vscode;code;editor;development;programming;ide;ai;

[Desktop Action new-empty-window]
Name=New Empty Window
Exec=antigravity --no-sandbox --new-window %F
Icon=antigravity
